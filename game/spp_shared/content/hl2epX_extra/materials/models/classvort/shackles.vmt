"vertexlitgeneric"
{
	"$baseTexture" "models/classvort/shackles"
	"$bumpmap" "models/classvort/shackles_normal"
	"$envmap" "env_cubemap"
	"$alphatest" 1
	"$normalmapalphaenvmapmask" 1
	"$envmaptint" "[ .0 1 .0 ]"
//	"$envmapcontrast" "1"
	"$nodecal" "1"
	"$model" "1"

        $detail detail\noise_detail_01 // e.g. for brickwork, concrete...
        $detailscale 7.74
        $detailblendfactor 0.8
        $detailblendmode 0


	"$phong"     "1"
//	"$phongexponent"  "10"
//	"$phongboost"   "4"
//	"$phongfresnelranges"   "[.5 .8 1]"
//	"$phongwarptexture"  "models/classvort/vort_iridescence"

	// Rim lighting parameters
	"$rimlight" "1"											// To enable rim lighting (requires phong)
	"$rimlightexponent" "5"										// Exponent for phong component of rim lighting	
	"$rimlightboost" "1"										                // Boost for ambient cube component of rim lighting
	"Proxies"
	{
		"IsNPC"
		{
			"scale" "4.0"
			"resultvar" "$selfillumtint"
		}
	}
}