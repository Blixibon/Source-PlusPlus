	"$reflectivity" "[.23 .31 .31 ]" //this is a artificial radiosity boost

		"$bumpmap" "Tile/tilefloor021a_normal"
		"$envmaptint" "[ .01 .01 .01]"
		"$normalmapalphaenvmapmask" 1
	"$phong" "1"

