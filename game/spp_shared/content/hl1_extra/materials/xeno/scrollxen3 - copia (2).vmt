"UnlitGeneric"
{
	// Original shader: BaseTexture
	"$basetexture" "xeno/scrollxen3"

	"$texscale"	4
	"$baseTextureOffset" "[0.5 0.5]"
	"Proxies"
	{
		"TextureScroll"
		{
			"texturescrollvar" "$basetexturetransform"
			"texturescrollrate" 2.00
			"texturescrollangle" 180.00
		}
		"TextureTransform"
		{
			"translateVar" "$baseTextureOffset"
			"scaleVar"     "$texscale"
			"resultVar"    "$baseTextureTransform"
		}
	}
}
