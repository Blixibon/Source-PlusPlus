"LightmappedGeneric"
{
	// Original shader: BaseTimesLightmapPlusMaskedSphericalEnvMap
	"$detail" "detail\metal_detail_01"
	"$detailscale" "2.5"
	"$detailblendfactor" ".65"
	"$detailblendmode" "0"
	"$basetexture" "halflife/duct_wall03"
	"$envmap" "environment maps/ductenv1"
	"$envmapmask" "halflife/duct_wall03r"
	"$basealphaenvmapmask" "1"
	"$envmapcontrast" 2
	"$envmaptint" "[ .4 .4 .4 ]"
	"$surfaceprop" "metalvent"

	"$texscale"	4
	"$baseTextureOffset" "[0.5 0.5]"
	"Proxies"
	{
		"TextureTransform"
		{
			"translateVar" "$baseTextureOffset"
			"scaleVar"     "$texscale"
			"resultVar"    "$baseTextureTransform"
		}
	}
}
