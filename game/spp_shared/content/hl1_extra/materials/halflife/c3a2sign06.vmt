"LightmappedGeneric"
{
	// Original shader: BaseTimesLightmapPlusMaskedSphericalEnvMap
	"$detail" "detail/detail_noise_metal"
	"$detailscale" 2.5
	"$basetexture" "halflife/c3a2sign06"
	"$envmapsphere" 1
	"$envmap" "environment maps/envmap001a"
	"$envmapmask" "halflife/c3a2sign06r"
	"$surfaceprop" "metal"

	"$texscale"	4
	"$baseTextureOffset" "[0.5 0.5]"
	"Proxies"
	{
		"TextureTransform"
		{
			"translateVar" "$baseTextureOffset"
			"scaleVar"     "$texscale"
			"resultVar"    "$baseTextureTransform"
		}
	}
}

