"LightmappedGeneric"
{
	// Original shader: BaseTimesLightmapPlusMaskedSphericalEnvMap
	"$detail" "detail/detail_noise1"
	"$detailscale" 2.5
	"$basetexture" "halflife/generic45"
	"$envmapsphere" 1
	"$envmap" "environment maps/envmap001a"
	"$envmapmask" "halflife/generic45r"
	"$surfaceprop" "glass"
     
        "$texscale" 4
        "$baseTextureOffset" "[0.5 0.5]"
        "$envmapOffset" "[0.5 0.5]"
    "Proxies"
     {
            "TextureTransform"
            {
   "translateVar" "$baseTextureOffset"
   "scaleVar"     "$texscale"
                   "resultVar"    "$baseTextureTransform"
            }
            "TextureTransform"
            {
   "translateVar" "$envmapOffset"
   "scaleVar"     "$texscale"
                   "resultVar"    "$envmapTransform"
            }
     }
}
