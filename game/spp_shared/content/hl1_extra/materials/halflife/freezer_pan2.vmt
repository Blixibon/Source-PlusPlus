"LightmappedGeneric"
{
	// Original shader: BaseTimesLightmapPlusMaskedSphericalEnvMap
	"$detail" "detail/detail_noise_metal"
	"$detailscale" 2.5
	"$basetexture" "halflife/freezer_pan2"
	"$surfaceprop" "metal"

	"$envmap" "environment maps/envmap001a"
	"$basealphaenvmapmask" "1"
	"$envmapcontrast" 1
	"$envmaptint" "[ .2 .2 .2 ]"

	"$texscale"	4
	"$baseTextureOffset" "[0.5 0.5]"
	"Proxies"
	{
		"TextureTransform"
		{
			"translateVar" "$baseTextureOffset"
			"scaleVar"     "$texscale"
			"resultVar"    "$baseTextureTransform"
		}
	}
}