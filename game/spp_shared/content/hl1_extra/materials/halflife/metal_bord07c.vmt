"LightmappedGeneric"
{
	// Original shader: BaseTimesLightmap
	"$detail" "detail/detail_noise_metal"
	"$detailscale" 2.5
	"$basetexture" "halflife/metal_bord07c"
	"$surfaceprop" "metal"
	"$envmap" "environment maps/envmap001a"
	"$basealphaenvmapmask" "1"
	"$envmapcontrast" 1
	"$envmaptint" "[ .7 .7 .7 ]"

	"$texscale"	8
	"$baseTextureOffset" "[0.5 0.5]"
	"Proxies"
	{
		"TextureTransform"
		{
			"translateVar" "$baseTextureOffset"
			"scaleVar"     "$texscale"
			"resultVar"    "$baseTextureTransform"
		}
	}
}