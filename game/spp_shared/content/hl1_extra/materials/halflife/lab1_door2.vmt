"LightmappedGeneric"
{
	// Original shader: BaseTimesLightmapPlusMaskedSphericalEnvMap
	"$detail" "detail/detail_noise_metal"
	"$detailscale" 2.5
	"$basetexture" "halflife/lab1_door2"
	"$envmap" "environment maps/envmap001a"
	"$basealphaenvmapmask" "1"
	"$envmapcontrast" 2
	"$envmaptint" "[ .1 .1 .1 ]"
	"$surfaceprop" "metal"
	"$envmapmask" "halflife/lab1_door2r"

	"$texscale"	4
	"$baseTextureOffset" "[0.5 0.5]"
	"Proxies"
	{
		"TextureTransform"
		{
			"translateVar" "$baseTextureOffset"
			"scaleVar"     "$texscale"
			"resultVar"    "$baseTextureTransform"
		}
	}
}

